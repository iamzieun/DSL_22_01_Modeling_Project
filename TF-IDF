     }\n","      </script>\n","    </div>\n","  </div>\n","  "]},"metadata":{},"execution_count":29}]},{"cell_type":"code","source":[""],"metadata":{"id":"K7MeBTfQReh4"},"execution_count":null,"outputs":[]}],"metadata":{"colab":{"collapsed_sections":[],"name":"220331 강의계획서 TFIDF + 라벨값 추가.ipynb","provenance":[],"mount_file_id":"1eSTr-_2ptUvTjMc7KclRX3kn8YhXG5iE","authorship_tag":"ABX9TyMcx6QdTybWfF5RSaUtDssU"},"kernelspec":{"display_name":"Python 3","name":"python3"},"language_info":{"name":"python"}},"nbformat":4,"nbformat_minor":0}